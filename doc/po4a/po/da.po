#: ../adduser.8:307
#, fuzzy
#| msgid "Use FILE instead of I</etc/adduser.conf>."
msgid ""
"Use I<file> instead of I</etc/adduser.conf>.  Multiple B<--conf> options can "
"be given."
msgstr "Brug FIL i stedet for I</etc/adduser.conf>."

